<Application x:Class="EcoTraveller.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EcoTraveller"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="lblFont" TargetType="{x:Type Control}">
            <Setter Property="FontFamily" Value="Fonts/Boldness.otf #Boldness"/>
        </Style>
    </Application.Resources>
</Application>
